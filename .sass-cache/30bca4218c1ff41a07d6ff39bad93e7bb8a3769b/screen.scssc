  	-webkit-border-bottom-left-radius: 10px;
  		-moz-border-top-left-radius: 10px;
  		-moz-border-bottom-left-radius: 10px;
  		border-top-left-radius: 10px;
  		border-bottom-left-radius: 10px;
 	 }
 	 .posicionIzquierdaAlta, .posicionIzquierdaBaja{
 	 	-webkit-border-top-right-radius: 10px;
 	 	-webkit-border-bottom-right-radius: 10px;
 	 		-moz-border-top-right-radius: 10px;
 	 		-moz-border-bottom-right-radius: 10px;
 	 		border-top-right-radius: 10px;
 	 		border-bottom-right-radius: 10px;
 	}
 	.posicionDerechaAlta{
 		top: 50px;
 		right: 0;
 	}
 	.posicionDerechaBaja{
 		bottom: 50px;
 		right: 0;
 	}
 	.posicionIzquierdaAlta{
 		top: 50px;
 		left: 0;
 	}
 	.posicionIzquierdaBaja{
 		bottom: 50px;
 		left: 0;
 		opacity: 0.85;
 	}
 	.listadoNodosInicio{
 		width: 720px;
 	}
 	.listadoNodosInicio .nodoLista{
 		width: 304px;
 		height: 440px;
 		float: left;
 		padding-top: 0;
 		padding-bottom: 0;
 		 border-bottom-style: none;
 	}
 	.listadoNodosInicio .nodoLista:first-child{
 		padding-right: 15px;
 		border-right: 1px solid #f2f2f2;
 		padding-left: 0;
 	}
 	.listadoNodosInicio .nodoLista:last-child{
 		border-right: 1px solid #f2f2f2;
 		padding: 0 15px;
 	}
 	.listadoNodosInicio .infoNodoLista{
 		width: 304px;
	 }
 	.listadoNodosInicio .nodoLista .imagenNodoLista{
 		margin: 0;
 		padding: 0;
 		height: 170px;
 		width: 305px;
 	}
 	.nodoLista .imagenNodoLista a:link, .nodoLista .imagenNodoLista a:visited{
	 	border-bottom-style: none;
	 }
 	.listadoNodosInicio .infoNodoLista .cabeceraNodo p{
 		margin-top: 10px;
 	}
 	#columnaTres{
 		height: 440px;
 		width: 304px;
 		float: right;
 		border-bottom: none;
 	}

 	p.tweetContenido{
 		margin-top: 40px;
 		padding-left: 20px;
 	}
 	p.tweetFecha{
 		text-align: right;
 		margin-top: 15px;
 	}
 	p.tweetInfo{
 		text-align: right;
 		margin-top: 5px;
 	}
 	a.nombreUsuario:link,  a.nombreUsuario:visited{
 		border-bottom-style: none;	
 	}
 /* @end */
 
 /* @group Pager */
 ul.pager{
 	margin: 15px 0 0;
	 padding: 0;
 }
  li.paginacion{
  	margin: 0;
	  padding: 0;
	  display: block;
  }
  li.paginacion.current{
  	background-color: #dfdfdf;
  	 padding: 4px 10px;
  	border-radius: 10px;
  	-webkit-border-radius: 10px;
  	-moz-border-radius: 10px;
	  color: white;
  }
 li.paginacion a:link,  li.paginacion a:visited{
 	border-bottom-style: none;
	 padding: 4px 10px;
	 border-radius: 10px;
	 -webkit-border-radius: 10px;
	 -moz-border-radius: 10px;
 }
 li.paginacion a:hover{
 	background-color: #ededed;
 }
 li.paginacion a:active{
 	background-color: #dfdfdf;
 }
 
 /* @end */
 
 /* @group ExtraInfoColaboradores */
 .colaboradorIcono{
 	cursor: pointer;
 }
 li div.subcategoryPopUp {
 	position: absolute;
 	z-index: 5;
 	font: 13px 'lucinda grande', Helvetica, Arial;
 	display: none;
 	width: 260px;
 	margin-top: 65px;
 	border: 1px solid rgba(66,66,66,0.8);
 	background-color: rgba(50,50,50,0.8);
 	border-radius: 5px;
 	-moz-border-radius: 5px;
 	-webkit-border-radius: 5px;
 	margin-left: 0;
 }
 .subcategoryPopUp .triangulo{
 	background-image: url(/imagenes/triangulo.png);
 	display: block;
 	width: 23px;
 	height: 13px;
 	position: absolute;
 	top: -12px;
 	left: 14px;
 	border-style: none;
 	border-width: 0;
 }
 .subcategoryPopUp h3{
 	margin-right: 20px;
	 margin-left: 10px;
	 margin-top: 8px;
	 position: relative;
 }
 .subcategoryPopUp a, .subcategoryPopUp a:link, .subcategoryPopUp a:active{
 	color: white;
 	display: block;
 	margin: 3px 20px 15px 10px;
	 border-bottom-style: none;
	 position: relative;
 }
 .subcategoryPopUp a:hover{
 	color: #bdbdbd;
 }
 .cuerpoPagina .addthis_toolbox{
	 float: left;
	 padding-right: 8px;
	 padding-top: 10px;
	 width: 80px;
 }
 .addthis_button_google_plusone at300b{
 	margin-left: 5px;
 }
 /* @end */
 /* @group bloque actividades */
 
 .bloqueActividades{
 	height: 170px;
 	width: 290px;
 	margin-left: 15px;
 }
 .bloqueActividades h2{
 	font: 25px/25px "colaborativaBook";
 	color: white;
 	background-color: #c7c7c7;
 	padding-left: 10px;
 	height: 30px;
 }
 .bloqueActividades{
 	font: 13px/16px "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
 	color: #343434;
 }
 .bloqueActividades ul{
 }
 .bloqueActividades li{
 box-sizing: border-box;
 -moz-box-sizing: border-box;
 -webkit-box-sizing: border-box;
 	border-bottom: 1px dotted silver;
 	height: 35px;
 	display: table;
 	width: 290px;
 }
 .bloqueActividades li a:link{
 box-sizing: border-box;
 -moz-box-sizing: border-box;
 -webkit-box-sizing: border-box;
 	height: 20px;
 	display: table-cell;
 	vertical-align: middle;
 	text-decoration: none;
 	color: #323232;
 	margin: 0;
	 text-align: left;
	 padding: 0;
	 border-bottom-style: none;
 }
 .bloqueActividades li a:visited{
 	text-decoration: none;
 	color: #323232;
 }
 .bloqueActividades li:hover{
 	background-color: #efefef;
 }
 .bloqueActividades li:active{
 	background-color: #d7d7d7;
 }
 .bloqueActividades li span{
 width: 33px;
 display: table-cell;
 position: relative;
 top: 0;
 left: 0;
 height: 33px;
 }
 .bloqueActividades li.estrategico span{
 		background: url(/imagenes/iconoEstrategias.png) 0 0px no-repeat;
 }
 .bloqueActividades li.acciones span{
 		background: url(/imagenes/iconoAcciones.png) 0 0px no-repeat;
 }
 .bloqueActividades li.geoposicionamiento span{
 		background: url(/imagenes/iconoGeoposicionamiento.png) 0 0px no-repeat;
 }
 .bloqueActividades li.visualizacion span{
 		background: url(/imagenes/iconoVisualizacion.png) 0 0px no-repeat;
 }
 
